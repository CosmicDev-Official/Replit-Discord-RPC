{
  "manifest_version": 3,
  "name": "Replit Discord RPC",
  "version": "1.0.0",
  "description": "Discord RPC for Replit web client.",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["*://*.replit.com/*"],
      "js": ["replit-rpc.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "pop.html"
  }
}
